<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for destination.membership -->
        <record id="seq_destination_membership" model="ir.sequence">
            <field name="name">Rental Order</field>
            <field name="code">destination.membership</field>
            <field name="prefix">M/%(y)s%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for destination.guest -->
        <record id="seq_destination_guest" model="ir.sequence">
            <field name="name">Guest Access Order</field>
            <field name="code">destination.guest</field>
            <field name="prefix">GA/%(y)s%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for destination.membership.member.activity -->
        <record id="seq_destination_membership_member_activity" model="ir.sequence">
            <field name="name">Member Access Order</field>
            <field name="code">destination.membership.member.activity</field>
            <field name="prefix">MA/%(y)s%(month)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
